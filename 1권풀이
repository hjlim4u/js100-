//34
const unsorted = prompt("키를 입력하세요");
let sorted = "";
sorted = unsorted
  .split(" ")
  .sort(function (a, b) {
    return a - b;
  })
  .join(" ");
if (unsorted === sorted) {
  console.log("Yes");
} else {
  console.log("No");
}
///35
function one(n) {
  function two(value) {
    const sq = Math.pow(value, n);
    return sq;
  }
  return two;
}
