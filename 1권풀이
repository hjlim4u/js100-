//40
const limit = prompt("입력: ");
const num = parseInt(prompt("입력: "));
let arr = [];
for (let i = 0; i < num; i++) {
  arr.push(parseInt(prompt("입력: ")));
}
let count = 0;
arr.sort((a, b) => a - b);
count = arr.reduce((prev, curr) => {
  if (prev + curr > limit) {
    return prev;
  }
  return prev + curr;
}, 0);

//41
function isPrime(num) {
  if (num <= 1) {
    return "No";
  }
  for (let i = 2; i < num; i++) {
    if (num % i === 0) {
      return "No";
    }
  }
  return "Yes";
}

